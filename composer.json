{
    "name": "tortlewortle/laravel-inertia-react-preset",
    "description": "A Laravel preset for scaffolding InertiaJS and React (+ tailwind).",
    "license": "MIT",
    "keywords": ["laravel", "laravel preset", "inertiajs", "inertia", "react", "reactjs", "tailwind"],
    "authors": [
        {
            "name": "TortleWortle",
            "email": "bas.kuipers@me.com"
        }
    ],
    "require": {
        "inertiajs/inertia-laravel": "^0.2.4",
        "illuminate/console": "^6.16",
        "illuminate/filesystem": "^6.16",
        "illuminate/support": "^6.16"
    },
    "autoload": {
        "psr-4": {
            "TortleWortle\\InertiaReactPreset\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TortleWortle\\InertiaReactPreset\\InertiaReactServiceProvider"
            ]
        }
    }
}
